{
  "name": "retro-green-theme-vscode",
  "displayName": "Retro Green Theme",
  "description": "A nostalgic theme inspired by vintage green monitor, binging a classic look to your VS CODE",
  "version": "1.1.6",
  "publisher": "LelinPadhan",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "color-theme",
    "retro",
    "green",
    "vintage",
    "monitor",
    "classic"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lelin07/retro-green-theme.git"
  },
  "scripts": {
    "test": "npm run test:manifest && npm run test:theme",
    "test:manifest": "node test/manifest.test.js",
    "test:theme": "node test/theme-json.test.js",
    "package:smoke": "vsce package --no-yarn --out /tmp/retro-green-theme-smoke.vsix",
    "ci": "npm run test && npm run package:smoke"
  },
  "icon": "assets/icon.png",
  "contributes": {
    "themes": [
      {
        "label": "Retro Green Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/retro-green-theme-color-theme.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.15.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "semantic-release": "^24.2.0"
  }
}
